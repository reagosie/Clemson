word(zeroproduct, 0)
load(x)
sub(one)
bge0(zeroloop)

label(zeroloop)
load(two)
mul(two)
store(zeroproduct)
load(x)
sub(one)
bgt0(zeroloop)

load(single)
mul(zeroproduct)
store(single)

label(caseone)
  word(y, 0)
  word(oneproduct, 0)
  load(y)
  sub(one)
  bge0(oneloop)

  label(oneloop)
  load(two)
  mul(two)
  store(oneproduct)
  load(y)
  sub(one)
  bgt0(oneloop)
